		clock-names = "main", "dma";
		clock-div = <1>;
	};

	i2c10: i2c@11db4000 {
		compatible = "mediatek,mt6983-i2c";
		reg = <0 0x11db4000 0 0x1000>,
			<0 0x11300800 0 0x80>;
		interrupts = <GIC_SPI 410 IRQ_TYPE_LEVEL_HIGH 0>;
		clocks = <&imp_iic_wrap2_clk CLK_IMP_AP_CLOCK_I2C10>,
