	};

	i2c11: i2c@11280000 {
		compatible = "mediatek,mt6983-i2c";
		reg = <0 0x11280000 0 0x1000>,
			<0 0x11300880 0 0x80>;
		interrupts = <GIC_SPI 411 IRQ_TYPE_LEVEL_HIGH 0>;
		clocks = <&imp_iic_wrap0_clk CLK_IMP_AP_CLOCK_I2C11>,
			<&pericfg_ao_clk CLK_PERAOP_APDMA>;
		clock-names = "main", "dma";
		clock-div = <1>;
