		reg = <1>;
		vddr-enable-gpios = <&pio 120 0>;
		vddr-aod-en-gpios = <&pio 22 0>;
		reset-gpios = <&pio 41 0>;
		//vmcldo-supply = <&mt6368_vmc>;
		vufsldo-supply = <&mt6363_vufs18>;
